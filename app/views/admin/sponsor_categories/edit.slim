= render :partial => 'users/admin_menu'

.col-lg-9
  h2 Редактировать категорию спонсоров
  = form_for @sponsor_category, url: admin_sponsor_category_path, method: :put do |f|
    = f.label 'Название категории (рус)'
    br
    = f.text_field :name
    br
    = f.label 'Название категории (eng)'
    br
    = f.text_field :name_en
    br
    = f.submit 'Подтвердить'